      }

      body[yahoo] .padding-left-20px {
        padding-left: 20px !important;
      }

      body[yahoo] .padding-right-20px {
        padding-right: 20px !important;
      }

      body[yahoo] .padding-top-10px {
        padding-top: 10px !important;
      }

      body[yahoo] .padding-top-20px {
        padding-top: 20px !important;
      }

      body[yahoo] .padding-top-30px {
        padding-top: 30px !important;
      }

      body[yahoo] .padding-top-195px {
        padding-top: 195px !important;
      }

      body[yahoo] .padding-top-250px {
        padding-top: 250px !important;
      }

      /* position */
      body[yahoo] .align-top-left {
        position: absolute !important;
        left: 0 !important;
        top: 0 !important;
      }

      body[yahoo] .align-top-340px-left {
        position: absolute !important;
        left: 0 !important;
        top: 340px !important;
      }

      /* font-sizes */
      body[yahoo] .font-size-13px {
        font-size: 13px !important;
      }

      body[yahoo] .font-size-14px {
        font-size: 14px !important;
